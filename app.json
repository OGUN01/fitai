{
  "expo": {
    "scheme": "fitai",
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "name": "FitAI",
    "slug": "fitness",
    "version": "0.1.2",
    "orientation": "portrait",
    "icon": "./assets/icon/1024.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/images/**/*",
      "assets/icon/**/*",
      "assets/splash.png",
      "assets/adaptive-icon.png",
      "assets/favicon.png"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fitai.fitness",
      "buildNumber": "11",
      "icon": "./assets/icon/1024.png"
    },
    "android": {
      "package": "com.fitai.fitness",
      "versionCode": 11,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/adaptive-icon.png",
        "backgroundImage": "./assets/icon/adaptive-icon.png",
        "backgroundColor": "#FFFFFF",
        "monochromeImage": "./assets/icon/adaptive-icon.png"
      },
      "icon": "./assets/icon/1024.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icon/512.png"
    },
    "extra": {
      "eas": {
        "projectId": "f40bedb5-ae93-45f9-a596-2f1aecbcf72d"
      },
      "router": {
        "origin": false
      }
    }
  }
}
